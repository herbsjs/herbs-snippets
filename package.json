{
  "name": "herbs-snippets",
  "displayName": "Herbs Snippets",
  "description": "This extension for Visual Studio Code adds snippets for HerbsJS",
  "version": "1.0.5",
  "publisher": "EndersonCosta",
  "icon": "docs/logo.jpg",
  "engines": {
    "vscode": "^1.45.0"
  },
  "scripts": {
    "lint": "npx eslint \"**/*.{js,jsx}\" --quiet",
    "lint:fix": "npx eslint \"**/*.{js,jsx}\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/herbs/herbs-snippets"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  },
  "keywords": [
    "herbsjs",
    "snippets",
    "herbs-snippets"
  ],
  "license": "MIT"
}
